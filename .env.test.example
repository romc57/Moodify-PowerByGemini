# Test Environment Variables
# Copy this file to .env.test and fill in your test API keys
# This file is used for integration tests that require real API access

# Gemini API Key for testing
# Get one from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Spotify Client ID for testing
# Get one from: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here

# Spotify Access Token for testing (optional, can be obtained via OAuth flow)
# If not provided, tests will skip integration tests requiring Spotify auth
SPOTIFY_ACCESS_TOKEN=your_spotify_access_token_here

# Spotify Refresh Token (optional, for token refresh testing)
SPOTIFY_REFRESH_TOKEN=your_spotify_refresh_token_here

# Spotify Redirect URI for testing (default: http://127.0.0.1:8081)
SPOTIFY_REDIRECT_URI=http://127.0.0.1:8081
